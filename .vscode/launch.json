{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "",
            "port": 40000,
            "host": "127.0.0.1",
            "showLog": true,
            "trace": "log",
            "logOutput": "rpc"
        },
        {
            "name": "Launch test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceRoot}/pkg/handlers",
            "args": [
                "-test.run",
                "TestAzadKubeProxyHandler"
            ],
            "envFile": "${workspaceFolder}/tmp/test_env",
        },
    ]
}